{
  "name": "highcharts-export-server",
  "author": "Highsoft AS <support@highcharts.com> (http://www.highcharts.com/about)",
  "license": "MIT",
  "type": "module",
  "version": "5.0.0",
  "main": "./dist/index.esm.js",
  "repository": {
    "url": "https://github.com/highcharts/node-export-server",
    "type": "git"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "bin",
    "dist",
    "lib",
    "msg",
    "public",
    "templates"
  ],
  "bin": {
    "highcharts-export-server": "./bin/cli.js"
  },
  "scripts": {
    "prestart": "rm -rf tmp || npx -y rimraf tmp && node ./node_modules/puppeteer/install.mjs",
    "start": "node ./bin/cli.js --enableServer true --logLevel 2",
    "start:dev": "npx nodemon ./bin/cli.js --enableServer true --logLevel 4",
    "start:debug": "node --inspect-brk=9229 ./bin/cli.js --enableDebug true --enableServer true --logLevel 4",
    "complete": "npm run lint && npm run format && npm run build",
    "lint": "eslint ./ --ext .js,.ts --fix",
    "format": "prettier ./ --config .prettierrc.cjs --write",
    "build": "rollup --config --silent",
    "prepack": "npm run build",
    "unit:test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky || true"
  },
  "dependencies": {
    "colors": "1.4.0",
    "cors": "^2.8.5",
    "dompurify": "3.1.6",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "https-proxy-agent": "^7.0.6",
    "jsdom": "^26.0.0",
    "multer": "^1.4.5-lts.1",
    "prompts": "^2.4.2",
    "puppeteer": "^24.2.0",
    "tarn": "^3.0.2",
    "uuid": "^11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "rollup": "^4.34.6"
  }
}
